<h1>New Upload</h1>
 
<%= form_for(@upload) do |f| %>
  <div>
    <%= f.label :name %><br>
    <%= f.file_field :name %>
  </div>
  <div>
    <%= f.label :directory %> <br>
    <%= f.select :directory, ['general', 'implementation','training','data quality']%>
  </div>
  <div>
    <%= f.submit %>
  </div>
<% end %>
 
<%= link_to 'Back', uploads_path %>